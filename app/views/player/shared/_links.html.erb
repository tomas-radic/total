<%- if controller_name != 'sessions' %>
  <%#= link_to "Prihlásiť sa", new_player_session_path(resource_name) %>
  <%= link_to "Prihlásiť sa", new_player_session_path %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%#= link_to "Registrácia", new_player_registration_path(resource_name) %>
  <%= link_to "Registrácia", new_player_registration_path %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%#= link_to "Zabudnuté heslo", new_player_password_path(resource_name) %>
  <%= link_to "Zabudnuté heslo", new_player_password_path %><br />
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%#= link_to "Neprišiel email na potvrdenie registrácie?", new_player_confirmation_path(resource_name) %>
  <%= link_to "Neprišiel email na potvrdenie registrácie?", new_player_confirmation_path %><br />
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%#= link_to "Neprišiel email na odomknutie?", new_player_unlock_path(resource_name) %>
  <%= link_to "Neprišiel email na odomknutie?", new_player_unlock_path %><br />
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%#= link_to "Prihlás sa cez #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), method: :post %>
    <%= link_to "Prihlás sa cez #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), method: :post %><br />
  <% end %>
<% end %>
